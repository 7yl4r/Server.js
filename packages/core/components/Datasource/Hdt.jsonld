        {
          "@id": "ldfc:Datasource/Hdt#file",
          "comment": "The HDT datasource source file",
          "range": "xsd:string",
          "unique": true
        },
        {
          "@id": "ldfc:Datasource/Hdt#external",
          "comment": "Switch to external HDT datasource if true",
          "range": "xsd:string",
          "unique": true
        }
      ],
      "constructorArguments": {
        "extends": "ldfc:Datasource#constructorArgumentsObject",
        "fields": [
          {
            "keyRaw": "file",
            "value": "ldfc:Datasource/Hdt#file"
          },
          {
            "keyRaw": "external",
            "value": "ldfc:Datasource/Hdt#external"
          }
        ]
      }
